{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputLogs": "errors-only"
    },
    "lint": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "src/**"
      ],
      "outputs": [],
      "outputLogs": "errors-only"
    },
    "typecheck": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "src/**"
      ],
      "outputs": [],
      "outputLogs": "errors-only"
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "inputs": [
        "src/**"
      ],
      "outputs": [],
      "outputLogs": "errors-only"
    }
  }
}
