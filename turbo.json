{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "docs": {
      "dependsOn": ["^docs"],
      "inputs": ["src/**"],
      "outputs": ["docs/**"]
    },
    "lint": {
      "dependsOn": ["build"],
      "inputs": ["src/**"],
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["build"],
      "inputs": ["src/**"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["src/**"],
      "outputs": []
    }
  }
}