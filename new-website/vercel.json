{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm export",
  "cleanUrls": true,
  "framework": null,
  "installCommand": "pnpm install",
  "routes": [
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ],
  "outputDirectory": "build",
  "trailingSlash": false
}
