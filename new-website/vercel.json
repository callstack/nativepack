{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm export",
  "cleanUrls": true,
  "framework": null,
  "installCommand": "pnpm install",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/404.html",
      "statusCode": 404
    }
  ],
  "outputDirectory": "build",
  "trailingSlash": false
}
