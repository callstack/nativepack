{
  "name": "@callstack/repack",
  "version": "4.0.0",
  "description": "A Webpack-based toolkit to build your React Native application with full support of Webpack ecosystem.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "react-native": "",
  "files": [
    "dist",
    "docs",
    "android",
    "!android/build",
    "ios",
    "!ios/build",
    "commands.js",
    "commands.d.ts",
    "client.js",
    "client.d.ts",
    "assets-loader.js",
    "flow-loader.js",
    "callstack-repack.podspec",
    "src/modules/ScriptManager/NativeScriptManager.ts",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/callstack/repack",
  "repository": "github:callstack/repack",
  "keywords": [
    "react-native",
    "react native",
    "webpack",
    "bundle",
    "bundling",
    "metro"
  ],
  "author": "zamotany <zamotany.oss@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build:js": "babel src --out-dir dist --extensions \".js,.ts\" --source-maps --ignore \"**/__tests__/**\" --delete-dir-on-start",
    "build:js:watch": "pnpm build:js --watch",
    "build:ts": "tsc --emitDeclarationOnly",
    "build:ts:watch": "pnpm build:ts --watch --preserveWatchOutput",
    "build": "pnpm build:js && pnpm build:ts",
    "docs": "typedoc && typedoc --options typedoc.client.json",
    "lint": "eslint --ext \".js,.ts\" commands.js client.js src",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "archive": "pnpm build && pnpm pack"
  },
  "peerDependencies": {
    "@react-native-community/cli": "*",
    "@react-native-community/cli-types": "*",
    "@rspack/core": "=>0.6.5",
    "@rspack/plugin-react-refresh": ">=0.6.5",
    "react-native": ">=0.74"
  },
  "dependencies": {
    "@callstack/repack-dev-server": "^4.0.0",
    "@discoveryjs/json-ext": "^0.5.7",
    "colorette": "^1.2.2",
    "dedent": "^0.7.0",
    "escape-string-regexp": "^4.0.0",
    "events": "^3.3.0",
    "execa": "^5.0.0",
    "flow-remove-types": "2.236.0",
    "fs-extra": "^10.0.0",
    "hasha": "^5.2.2",
    "image-size": "^1.1.1",
    "jsonwebtoken": "^9.0.0",
    "lodash.throttle": "^4.1.1",
    "memfs": "^4.9.1",
    "mime-types": "^2.1.35",
    "pretty-format": "^26.6.2",
    "react-refresh": "^0.14.0",
    "schema-utils": "^3.0.0",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.17.12",
    "@babel/plugin-transform-modules-commonjs": "^7.23.2",
    "@react-native-community/cli": "13.6.4",
    "@react-native-community/cli-types": "13.6.4",
    "@rspack/core": "0.6.5",
    "@rspack/plugin-react-refresh": "0.6.5",
    "@swc/helpers": "0.5.11",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash.throttle": "^4.1.9",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18",
    "@types/react-dom": "^17.0.7",
    "@types/shallowequal": "^1.1.1",
    "babel-jest": "^28.1.1",
    "eslint": "^8.53.0",
    "headers-polyfill": "^3.0.7",
    "jest": "^28.1.1",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "rspack-plugin-virtual-module": "^0.1.12",
    "typedoc": "^0.25.3",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.2.2"
  },
  "codegenConfig": {
    "name": "RNScriptManagerSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  }
}
